{
  "name": "ember-modifier",
  "version": "3.0.0",
  "description": "A library for writing Ember modifiers",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/ember-modifier/ember-modifier",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "lint:ts": "tsc --noEmit",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:hbs": "ember-template-lint .",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "start": "ember serve",
    "test": "npm-run-all test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "pretest": "npm run prepublishOnly",
    "posttest": "npm run clean",
    "test:types": "ember try:each --config-path=./config/ember-try-typescript.js",
    "prepublish:types": "ember ts:precompile",
    "prepublishOnly": "ember ts:precompile",
    "clean": "ember ts:clean",
    "postpublish": "npm run clean"
  },
  "dependencies": {
    "ember-cli-babel": "^7.26.6",
    "ember-cli-normalize-entity-name": "^1.0.0",
    "ember-cli-string-utils": "^1.1.0",
    "ember-cli-typescript": "^4.2.1",
    "ember-compatibility-helpers": "^1.2.5"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.15.8",
    "@ember/optional-features": "^2.0.0",
    "@babel/preset-typescript": "^7.15.0",
    "@ember/test-helpers": "^2.5.0",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "@types/ember": "^3.16.5",
    "@types/ember-qunit": "^3.4.15",
    "@types/ember-resolver": "^5.0.10",
    "@types/ember__test-helpers": "^2.0.2",
    "@types/qunit": "^2.11.2",
    "@types/rsvp": "^4.0.4",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^2.2.3",
    "ember-cli": "^4.0.1",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-htmlbars": "^6.0.1",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-decorators-polyfill": "^1.1.5",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^1.0.0",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.3",
    "ember-source": "^3.28.4",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.10.0",
    "ember-try": "^1.4.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.7",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "expect-type": "^0.13.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "qunit": "^2.17.2",
    "qunit-dom": "^2.0.0",
    "release-it": "^14.11.6",
    "release-it-lerna-changelog": "^4.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.2",
    "webpack": "^5.58.2"
  },
  "engines": {
    "node": "12.* || >= 14"
  },
  "prettier": {
    "singleQuote": true
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  },
  "types": "index.d.ts"
}
