{
  "name": "ember-modifier",
  "version": "4.0.0-beta.1",
  "description": "A library for writing Ember modifiers",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/ember-modifier/ember-modifier",
  "license": "MIT",
  "author": "",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*",
    "./addon-main.js": "./addon-main.js"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "*": [
        "./dist/*"
      ],
      "./-private/signature": [
        "./dist/-private/signature.d.ts"
      ]
    }
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "files": [
    "addon-main.js",
    "blueprints",
    "dist"
  ],
  "scripts": {
    "build": "rollup --config",
    "lint": "eslint . --cache --ext .ts,.js",
    "lint:fix": "eslint . --fix --ext .ts,.js",
    "prepublishOnly": "rollup --config",
    "start": "rollup --config --watch",
    "test": "echo 'Please refer to test-app for addon tests'",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.7.1",
    "ember-cli-normalize-entity-name": "^1.0.0",
    "ember-cli-string-utils": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@babel/eslint-parser": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-proposal-decorators": "^7.17.12",
    "@babel/preset-typescript": "^7.17.12",
    "@embroider/addon-dev": "^1.7.1",
    "@rollup/plugin-babel": "^5.3.1",
    "@types/ember-qunit": "~5.0.0",
    "@types/ember-resolver": "^5.0.10",
    "@types/ember__debug": "~4.0.1",
    "@types/ember__destroyable": "~4.0.0",
    "@types/ember__modifier": "~4.0.1",
    "@types/ember__test-helpers": "^2.0.2",
    "@types/qunit": "^2.11.2",
    "@types/rsvp": "^4.0.4",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "ember-destroyable-polyfill": "^2.0.3",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "expect-type": "^0.13.0",
    "prettier": "^2.4.1",
    "rollup": "^2.74.1",
    "rollup-plugin-ts": "^2.0.7",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  },
  "peerDependencies": {
    "ember-source": "*"
  },
  "peerDependenciesMeta": {
    "ember-source": {
      "optional": true
    }
  },
  "engines": {
    "node": "14.* || 16.* || >= 18"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "main": "addon-main.js",
    "type": "addon",
    "version": 2
  },
  "volta": {
    "extends": "../package.json"
  }
}
