{
  "name": "ember-modifier",
  "version": "4.0.0-beta.1",
  "description": "A library for writing Ember modifiers",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/ember-modifier/ember-modifier",
  "license": "MIT",
  "author": "",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*",
    "./addon-main.js": "./addon-main.js"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "*": [
        "./dist/*"
      ],
      "./-private/signature": [
        "./dist/-private/signature.d.ts"
      ]
    }
  },
  "files": [
    "addon-main.js",
    "blueprints",
    "dist"
  ],
  "scripts": {
    "build": "rollup --config",
    "lint": "eslint . --cache --ext .ts,.js",
    "lint:fix": "eslint . --fix --ext .ts,.js",
    "prepublishOnly": "rollup --config",
    "start": "rollup --config --watch",
    "test": "echo 'A v2 addon does not have tests, run tests in test-app'",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.7.1",
    "ember-cli-normalize-entity-name": "^1.0.0",
    "ember-cli-string-utils": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.20.2",
    "@babel/plugin-syntax-decorators": "^7.17.0",
    "@babel/preset-typescript": "^7.18.6",
    "@embroider/addon-dev": "^2.0.0",
    "@tsconfig/ember": "^1.0.0",
    "@types/ember-resolver": "^5.0.13",
    "@types/ember__application": "^4.0.4",
    "@types/ember__debug": "^4.0.3",
    "@types/ember__destroyable": "^4.0.1",
    "@types/ember__modifier": "^4.0.3",
    "@types/ember__owner": "^4.0.2",
    "@types/rsvp": "^4.0.4",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.44.0",
    "ember-cli-htmlbars": "^6.1.1",
    "ember-destroyable-polyfill": "^2.0.3",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ember": "^11.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "expect-type": "^0.15.0",
    "prettier": "^2.7.1",
    "rollup": "^3.2.5",
    "rollup-plugin-ts": "^3.0.2",
    "typescript": "^4.9.3"
  },
  "peerDependencies": {
    "ember-source": "*"
  },
  "peerDependenciesMeta": {
    "ember-source": {
      "optional": true
    }
  },
  "engines": {
    "node": "14.* || 16.* || >= 18"
  },
  "volta": {
    "extends": "../package.json"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.js",
    "versionCompatibility": {
      "ember": "3.24 || >=4.0"
    }
  }
}
